{
  "extends": "@react-native/typescript-config",
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "**/node_modules",
    "**/Pods",
    "harmony"
  ],
  "compilerOptions": {
    "lib": [
      "esnext",
      "dom"
    ],
    "noUnusedLocals": true,
    "baseUrl": "./src",
    "verbatimModuleSyntax": true,
    "paths": {
      "react-native-fs": [
        "../node_modules/@react-native-ohos/react-native-fs"
      ],
      "@react-native-async-storage/async-storage": [
        "../node_modules/@react-native-ohos/async-storage"
      ],
      "@react-native-cookies/cookies": [
        "../node_modules/@react-native-ohos/cookies"
      ],
      "react-native-blob-util": [
        "../node_modules/@react-native-ohos/react-native-blob-util"
      ],
      "react-native-gesture-handler": [
        "../node_modules/@react-native-ohos/react-native-gesture-handler"
      ],
      "react-native-webview": [
        "../node_modules/@react-native-ohos/react-native-webview"
      ],
      "@react-navigation/elements": [
        "../node_modules/@react-native-oh-tpl/elements"
      ],
      "@react-navigation/native-stack": [
        "../node_modules/@react-native-oh-tpl/native-stack"
      ],
      "react-native-safe-area-context": [
        "../node_modules/@react-native-oh-tpl/react-native-safe-area-context"
      ],
      "@react-navigation/stack": [
        "../node_modules/@react-native-oh-tpl/stack"
      ],
      "react-native-localize": [
        "../node_modules/@react-native-ohos/react-native-localize"
      ],
      "react-native-pager-view": [
        "../node_modules/@react-native-oh-tpl/react-native-pager-view"
      ],
      "react-native-pdf": [
        "../node_modules/@react-native-ohos/react-native-pdf"
      ],
      "react-native-reanimated": [
        "../node_modules/@react-native-ohos/react-native-reanimated"
      ],
      "react-native-share": [
        "../node_modules/@react-native-ohos/react-native-share"
      ],
      "react-native-tab-view": [
        "../node_modules/@react-native-oh-tpl/react-native-tab-view"
      ]
    }
  }
}
